{
  "name": "n8n-nodes-housecall-pro",
  "version": "1.0.0",
  "description": "A comprehensive n8n community node for Housecall Pro field service management providing 10 resources and 60+ operations for customer management, job scheduling, invoicing, and payment processing.",
  "license": "BUSL-1.1",
  "author": {
    "name": "Velocity BPA",
    "email": "licensing@velobpa.com",
    "url": "https://velobpa.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Velocity-BPA/n8n-nodes-housecall-pro.git"
  },
  "homepage": "https://github.com/Velocity-BPA/n8n-nodes-housecall-pro",
  "bugs": {
    "url": "https://github.com/Velocity-BPA/n8n-nodes-housecall-pro/issues"
  },
  "keywords": [
    "n8n",
    "n8n-community-node-package",
    "housecall-pro",
    "housecallpro",
    "field-service",
    "hvac",
    "plumbing",
    "electrical",
    "home-services",
    "scheduling",
    "invoicing",
    "job-management",
    "service-management"
  ],
  "main": "dist/nodes/HousecallPro/HousecallPro.node.js",
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/HousecallProApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/HousecallPro/HousecallPro.node.js",
      "dist/nodes/HousecallPro/HousecallProTrigger.node.js"
    ]
  },
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.ts\"",
    "format:check": "prettier --check \"**/*.ts\"",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "npm run build",
    "clean": "rimraf dist"
  },
  "files": [
    "dist",
    "LICENSE",
    "COMMERCIAL_LICENSE.md",
    "LICENSING_FAQ.md",
    "README.md"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.19",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "gulp": "^4.0.2",
    "jest": "^29.7.0",
    "n8n-workflow": "^1.30.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "n8n-workflow": "^1.30.0"
  }
}
